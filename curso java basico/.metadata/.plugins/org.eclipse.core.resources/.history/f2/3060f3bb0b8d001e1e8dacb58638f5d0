package aula11;

public class VariaveisInteiros {

	public static void main(String[] args) {
		byte idade01 = 20;
		short idade02 = 21;
		int idade03 = 22;
		long idade04 = 23;
		
		System.out.println("Valor variaveis idade01: " +idade01);
		System.out.println("Valor variaveis idade02: " +idade02);
		System.out.println("Valor variaveis idade03: " +idade03);
		System.out.println("Valor variaveis idade04: " +idade04);

	}

}
